<openerp>
    <data>
        <!-- FORMVIEW -->
        <record id="view_fiscal_printer_wiz_update_printers_form" model="ir.ui.view">
            <field name="name">fiscal_printer.wiz_update_printers.form</field>
            <field name="model">fiscal_printer.wiz_update_printers</field>
            <field name="arch" type="xml">
                <form string="wiz_update_printers" version="7.0">
                    <header>
                        <button name="sgn_do_create"
                            states="listing"
                            string="do_create"
                            />
                        <field name="state" widget="statusbar"
                            statusbar_colors='{}'
                            />
                    </header>
                    <sheet string="wiz_update_printers">
			    <p>Fiscal Printers</p>
			    <field name="wiz_fiscal_printer_ids">
				    <tree>
					    <field name="name"/>
					    <field name="protocol"/>
					    <field name="model"/>
					    <field name="serialNumber"/>
				    </tree>
			    </field>
                    </sheet>

                </form>
            </field>
        </record>
    </data>
</openerp>
